{
  "name" : "curfle/package-leuchtturm",
  "type": "library",
  "description": "Package that supports the creation of a GraphQL api within a curfle project",
  "keywords": [],
  "require": {
    "php": ">=8.0",
    "curfle/framework": "~0",
    "joonlabs/php-graphql": "~2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Leuchtturm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Examples\\": "examples/"
    }
  },
  "scripts": {
    "test": "phpunit tests"
  }
}
